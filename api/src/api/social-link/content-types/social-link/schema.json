{
  "kind": "collectionType",
  "collectionName": "social_links",
  "info": {
    "singularName": "social-link",
    "pluralName": "social-links",
    "displayName": "Social Link",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "url": {
      "type": "string",
      "required": true
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "background_color": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "required": true,
      "customField": "plugin::color-picker.color"
    }
  }
}